<script type="text/javascript"> 
 
// var server = 'http://10.42.0.1:3000';
 var server = 'http://localhost:3000';
 
//
require(['text!../views/footer.ejs', 'ejs'], function(thetemplate) { 
	$('.page-footer').html(ejs.render(thetemplate, { items: 0 }));
});
 
 /***/
 var render = function(template, remote, selector){
  	
  	require(['text!../views/'+template+'.ejs', 'ejs'], function(thetemplate) {
  		if(remote){        
				$.ajax({
		      type: "get",
		      beforeSend: function(xhr){
		  				if (xhr.overrideMimeType){
						    xhr.overrideMimeType("application/json");
		  				}
					},
		      url: server+'/json/'+template,
		      dataType: 'json',
		      success: function(data){  
						var html = ejs.render(thetemplate, { items: data });
						$(selector).html(html);
						
					},
					error : function(e){ alert('ERROR'+template+'__'+JSON.stringify(e)); }		  
				});
			
			}else{
				var html = ejs.render(thetemplate, {items:0});
				$(selector).html(html);
			}	
		});
 	}
 	

//render('film', false, '#page');
render('videos', true, '#page');
 
</script> 	

