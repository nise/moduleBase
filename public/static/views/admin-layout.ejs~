<script type="text/javascript"> 
 
/*
The following lines need to be changed regarding the underlaying server
**/ 

var server = '';

 
	 	
/***/
var render = function(template, remote, selector){ 
  	require(['text!../views/'+template+'.ejs', 'ejs'], function(thetemplate) {  
  		if(remote){         
				$.ajax({
		      type: "get",
		      beforeSend: function(xhr){
		  				if (xhr.overrideMimeType){
						    xhr.overrideMimeType("application/json");
		  				}
					},
		      url: server+'/json/'+template,
		      dataType: 'json',
		      success: function(data){ // alert(JSON.stringify(data)) 
						var html = ejs.render(thetemplate, { items: data });
						$(selector).html(html);
						
					},
					error : function(e){ alert('ERROR at '+template+':'+JSON.stringify(e)); }				  
				});
			
			}else{ 
				var html = ejs.render(thetemplate, {items:0});
				$(selector).html(html);
			}	
	}); // end require
}	// end render

jQuery(document).ready(function(e) { 
	var t = render('user-data', true, '.user-area'); 
	//var	socket = io.connect('http://127.0.0.1'); 
	//socket.emit('registered user', { user_id: 22, group_id: 99 });
		
